//Programa que pide dos números y determina cual es mayor
#include <iostream>
using namespace std:
int main() {
  int a,b;
  cout << "Ingrese un número:" << endl;
  cin << a;
  cout << "Ingrese un número:" << endl;
  cin << b;
  if (a > b) {
    cout << a << " es mayor que " << b;
  } else {
    cout << b << " es mayor que " << a;
  }
  return 0;
}
