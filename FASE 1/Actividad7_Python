class Persona:
    def __init__(self):
        self.nombre = ""
        self.edad = 0

    def setNombre(self, nombre):
        self.nombre = nombre

    def getNombre(self):
        return self.nombre

    def setEdad(self, edad):
        self.edad = edad

    def getEdad(self):
        return self.edad
if __name__ == "__main__":
    per1 = Persona()
    per1.setNombre("Luis")
    print(per1.getNombre())
    per1.setEdad(22)
    print(per1.getEdad())
