#include <iostream>

using namespace std;

int factorial(int n){
    int fact = 1;
    for (int i = 1; i <= n; i++){
        fact *= i;
    }
    return fact;
}

int main(){
    int num;
    cout << "Ingrese un nÃºmero: " << endl;
    cin >> num;
    cout << "El factorial es: " << factorial(num);
    return 0;
}
